---
interface Props {
  class?: string;
}

const { class: className = "w-6 h-6" } = Astro.props;
---

<svg class={className} fill="none" stroke="currentColor" viewBox="0 0 24 24">
  <path
    id="menuIcon"
    d="M4 6h16M4 12h16M4 18h16"
    stroke-width="2"
    stroke-linecap="round"></path>
  <path
    id="closeIcon"
    class="hidden"
    d="M6 18L18 6M6 6l12 12"
    stroke-width="2"
    stroke-linecap="round"></path>
</svg>
